#include <stdio.h>

int main(void){
    int N, R;
    printf("Defina a quantidade de elementos: ");
    scanf("%d", &N);

    int vetor[N];
    for(int i=0; i<N; i++){
        printf("Escolha o %dº elemento: ", i+1);
        scanf("%d", vetor+i);
    }

    printf("Escolha um número de referência: ");
    scanf("%d", &R);
    

    
    return 0;
}